{
	"private": true,
	"name": "berlin-bicycle-counters-map",
	"description": "Show Berlin bicycle counters on a map.",
	"version": "0.1.0",
	"main": "index.js",
	"keywords": [
		"bicycle",
		"counters",
		"cycling",
		"bike",
		"berlin"
	],
	"author": "Jannis R <mail@jannisr.de>",
	"homepage": "https://github.com/derhuerst/berlin-bicycle-counters-map",
	"repository": "derhuerst/berlin-bicycle-counters-map",
	"bugs": "https://github.com/derhuerst/berlin-bicycle-counters-map/issues",
	"license": "ISC",
	"engines": {
		"node": ">=6"
	},
	"dependencies": {
		"approximate-number": "^2.0.0",
		"color-interpolate": "^1.0.2",
		"fetch-ponyfill": "^4.1.0",
		"lodash": "^4.17.4",
		"mapbox-gl": "^0.40.0",
		"p-map": "^1.2.0",
		"pinkie-promise": "^2.0.1",
		"query-string": "^5.0.0"
	},
	"scripts": {
		"start": "watchify -t [ babelify --presets env ] index.js -o bundle.min.js",
		"bundle": "env NODE_ENV=production browserify -t [ babelify --presets env ] index.js > bundle.js",
		"minify": "uglifyjs -mc --screw-ie8 -- bundle.js > bundle.min.js",
		"build": "npm run bundle && npm run minify"
	},
	"devDependencies": {
		"babel-preset-env": "^1.6.0",
		"babelify": "^7.3.0",
		"browserify": "^14.4.0",
		"uglify-es": "^3.1.0",
		"watchify": "^3.9.0"
	}
}
